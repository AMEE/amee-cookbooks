[package]
id = pdfserver

[install]
is_configured = true
state = enabled

[ui]
is_visible = true
label = PDF Server for Linux
# Changed is_visible from false to true

[launcher]
author = Splunk, Inc.
description = The PDF Server for Linux add-on allows you to create a PDF formatted report from any dashboard, view, search or report within Splunk (WYSIWYG) and schedule the delivery of these reports via email. This add-on requires that you have at least one instance of Splunk on a Linux host on your network configured to serve as the PDF Server. This add-on only works with a Splunk Enterprise license
version = 1.3
